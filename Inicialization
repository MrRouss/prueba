void crearTablero(){//No me creo que esto de, aun no lo he compilado
    char tablero[][];
    for(int i=0; i<FILAS+1; i++){
        for(int j=0; j<COLUMNAS+1; j++){
            tablero[i][j]=NULL;
            printf("%d,%d:%s \n", i, j tablero[i][j]);
        }}
    
}

